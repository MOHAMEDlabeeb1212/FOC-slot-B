#include <stdio.h>

int main() {
    int num;

    printf("Enter an integer: ");
    scanf("%d", &num);

    if (num > 0) {
        printf("%d is a positive number.\n", num);
    } else if (num < 0) {
        printf("%d is a negative number.\n", num);
    } else {
        printf("The number is zero.\n");
    }

    return 0;
}











exp:24

#include <stdio.h>

int main() {
    int num1, num2, temp;

    printf("Enter the first number: ");
    scanf("%d", &num1);

    printf("Enter the second number: ");
    scanf("%d", &num2);

    
    temp = num1;
    num1 = num2;
    num2 = temp;

    printf("After swapping:\n");
    printf("First number: %d\n", num1);
    printf("Second number: %d\n", num2);

    return 0;
}














exp:25

#include <stdio.h>

int main() {
    int num1, num2;

    printf("Enter the first number: ");
    scanf("%d", &num1);

    printf("Enter the second number: ");
    scanf("%d", &num2);

    // Swapping without a temporary variable
    num1 = num1 + num2;
    num2 = num1 - num2;
    num1 = num1 - num2;

    printf("After swapping:\n");
    printf("First number: %d\n", num1);
    printf("Second number: %d\n", num2);

    return 0;
}



exp:26

#include <stdio.h>

int main() {
    int n, num, largest;

    printf("Enter the number of integers: ");
    scanf("%d", &n);

    printf("Enter an integer: ");
    scanf("%d", &largest);

    for (int i = 1; i < n; i++) {
        printf("Enter another integer: ");
        scanf("%d", &num);

        if (num > largest) {
            largest = num;
        }
    }

    printf("The largest number is: %d\n", largest);

    return 0;
}




exp:27

#include <stdio.h>
#include <math.h>

int main() {
    double x, sum = 0.0;
    int n;

    printf("Enter the value of x in radians: ");
    scanf("%lf", &x);

    printf("Enter the number of terms (n): ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        double term = pow(-1, i) * (pow(x, 2 * i + 1) / tgamma(2 * i + 2));
        sum += term;
    }

    printf("sin(%.2f) = %.10f\n", x, sum);

    return 0;
}


exp:28

#include <stdio.h>
#include <math.h>

int main() {
    double x, sum = 0.0;
    int n;

    printf("Enter the value of x in radians: ");
    scanf("%lf", &x);

    printf("Enter the number of terms (n): ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        double term = pow(-1, i) * (pow(x, 2 * i) / tgamma(2 * i + 1));
        sum += term;
    }

    printf("cos(%.2f) = %.10f\n", x, sum);

    return 0;
}


exp:29

#include <stdio.h>
#include <math.h>

int main() {
    double x, sum = 1.0;
    int n;

    printf("Enter the value of x: ");
    scanf("%lf", &x);

    printf("Enter the number of terms (n): ");
    scanf("%d", &n);

    double term = 1.0;
    for (int i = 1; i < n; i++) {
        term *= -x / i;
        sum += term;
    }

    printf("e^(-%.2f) = %.10f\n", x, sum);

    return 0;
}





exp:30

#include <stdio.h>

int linearSearch(int arr[], int size, int target) {
    for (int i = 0; i < size; i++) {
        if (arr[i] == target) {
            return i; // Return the index where the element was found
        }
    }
    return -1; 

int main() {
    int size, target;

    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int arr[size];
    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the element to search: ");
    scanf("%d", &target);

    int index = linearSearch(arr, size, target);

    if (index != -1) {
        printf("Element %d found at index %d.\n", target, index);
    } else {
        printf("Element %d not found in the array.\n", target);
    }

    return 0;
}






