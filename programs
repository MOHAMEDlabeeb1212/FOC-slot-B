int main() 
{
    int i,n;
    printf("enter the number of odd numbers to generate: ");
    scanf("%d", &n);
    printf("the first %d odd numbers are:\n", n);
    for (i=1;i<=n;i++)
	 {
        printf("%d ", 2*i-1);
    }
    printf("\n");
printf("mohamed labeeb, 192211344");
    
    return 0;
}





EXP:6

#include <stdio.h>

int main() {
    int n, sum = 0;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        sum += i;
    }

    printf("Sum of the series 1 + 2 + 3 + ... + %d = %d\n", n, sum);
    printf("mohamed labeeb, 192211344");
    return 0;
}









