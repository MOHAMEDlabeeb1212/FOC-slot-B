{
    int sum;
    int a;
    printf("enter a number :");
    scanf("%d",&a);
    for (int i=1;i<=a;i+=1)
    {
        if (i%2==0)
            sum+=i;
    }
    printf("the sum is :%d",sum);
printf("mohamed labeeb, 1992211344");
    return 0;
}







