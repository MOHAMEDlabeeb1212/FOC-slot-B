EXP:16
#include <stdio.h>

int main() {
    int n;
    unsigned long long factorial = 1;

    printf("Enter a positive integer: ");
    scanf("%d", &n);

    if (n < 0) {
        printf("Factorial is not defined for negative numbers.\n");
    } else {
        for (int i = 1; i <= n; i++) {
            factorial *= i;
        }

        printf("Factorial of %d = %llu\n", n, factorial);
    }
    printf("labeeb, 192211344"); 
    return 0;
}










EXP;17
#include <stdio.h>
#include <math.h>

int main() {
    int num, originalNum, remainder, n = 0;
    double result = 0.0;

    printf("Enter an integer: ");
    scanf("%d", &num);

    originalNum = num;

    while (originalNum != 0) {
        originalNum /= 10;
        ++n;
    }

    originalNum = num;

   
    while (originalNum != 0) {
        remainder = originalNum % 10;
        result += pow(remainder, n);
        originalNum /= 10;
    }

    if ((int)result == num) {
        printf("%d is an Armstrong number.\n", num);
    } else {
        printf("%d is not an Armstrong number.\n", num);
    }
printf("labeeb, 192211344");
    return 0;
}







EXP;18
#include <stdio.h>

int main() {
    int n, num;
    double sum = 0.0;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter %d numbers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &num);
        sum += num;
    }

    double average = sum / n;

    printf("Sum = %.2f\n", sum);
    printf("Average = %.2f\n", average);
   
    printf("labeeb, 192211344");
    return 0;
}








EXP:19
#include <stdio.h>

int main() {
    int num, digit;

    printf("Enter an integer: ");
    scanf("%d", &num);

    printf("Digits of %d are: ", num);

    // Extract and print each digit
    while (num > 0) {
        digit = num % 10;
        printf("%d ", digit);
        num /= 10;
    }

    printf("\n");
    printf("labeeb, 192211344");
    return 0;
}







EXP:20:

#include <stdio.h>

int main() {
    int num, digit, sum = 0;

    printf("Enter an integer: ");
    scanf("%d", &num);

    int originalNum = num; // Save the original number for printing later

    
    while (num > 0) {
        digit = num % 10;
        sum += digit;
        num /= 10;
    }

    printf("Sum of the digits of %d = %d\n", originalNum, sum);
    printf("labeeb, 192211344"); 
    return 0;
}














exp:21
#include <stdio.h>

int main() {
    int num, reversed = 0, digit;

    printf("Enter an integer: ");
    scanf("%d", &num);

    int originalNum = num; // Save the original number for printing later

    // Reverse the digits
    while (num != 0) {
        digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }

    printf("Original number: %d\n", originalNum);
    printf("Reversed number: %d\n", reversed);

    return 0;
}















