EXP:9

#include <stdio.h>

int main() {
    int n, sum = 0;

    printf("Enter the value of N: ");
    scanf("%d", &n);

    int sign = 1; // To alternate the sign (+ or -)

    for (int i = 1; i <= n; i++) {
        sum += sign * i;
        sign = -sign; // Change the sign for the next iteration
    }

    printf("Sum of the series 1 - 2 + 3 - 4 + 5 ... %d = %d\n", n, sum);
    printf(""labeeb, 199211344);
    return 0;
}











